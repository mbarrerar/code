<%= simple_form_for([:admin, space, deploy_key], :url => form_url) do |form| %>
    <%= form.input(:name, :label => "Key Name") %>
    <%= form.input(:key, :input_html => { :cols => 50, :rows => 15 }) %>
    <%= form.button(:submit) %> or
    <%= link_to_cancel(admin_space_deploy_keys_path(space)) %>
<% end %>
